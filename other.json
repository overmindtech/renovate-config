{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "AWS RDS Aurora PostgreSQL Serverless",
      "groupSlug": "rds-aurora-docker",
      "matchDatasources": ["docker"],
      "matchPackageNames": ["postgres"],
      "allowedVersions": "/^14\\.6-alpine$/"
    },
    {
      "groupName": "Alpine-based Docker images",
      "groupSlug": "alpine-docker",
      "matchDatasources": ["docker"],
      "matchPackageNames": ["alpine", "golang"]
    },
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["golang"],
      "extractVersion": "^(?<version>\\d+\\.\\d+-alpine\\d.\\d)"
    }
  ]
}
