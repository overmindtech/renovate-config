{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "AWS RDS Aurora PostgreSQL Serverless",
      "groupSlug": "rds-aurora-docker",
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "postgres"
      ],
      "allowedVersions": "/^14\\.9-alpine$/"
    },
    {
      "groupName": "Docker Images",
      "groupSlug": "docker",
      "matchDatasources": [
        "docker"
      ]
    },
    {
      "groupName": "Terraform",
      "groupSlug": "terraform",
      "matchDatasources": [
        "terraform-module",
        "terraform-provider"
      ]
    }
  ]
}